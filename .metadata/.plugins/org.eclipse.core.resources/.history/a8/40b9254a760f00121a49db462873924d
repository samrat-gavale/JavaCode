package career_cup;

public class PrintSpiral {
	public static void printRange(int arr[][], int strt, int end, int constant, int rc){
		int i;
		if(strt<=end){
			if(rc == 0)
				for(i=strt;i<=end;i++)
					System.out.print(" " + arr[constant][i]);
			else
				for(i=strt;i<=end;i++)
					System.out.print(" " + arr[i][constant]);
				
		}
		else{
			
		}
	}
}
